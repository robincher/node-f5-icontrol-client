{
  "name": "node-icontrol-client",
  "description": "F5 iControl REST API client for Node.js",
  "version": "0.0.4",
  "private": true,
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": "^10.x.x"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "keywords": [
    "node",
    "f5",
    "iControl"
  ],
  "license": "GPL-3.0",
  "scripts": {
    "test": "BLUEBIRD_DEBUG=1 mocha --opts ./spec/support/mocha.opts $(find ./spec/ -name '*Spec.js')",
    "testCoverage": "nyc --reporter=lcovonly npm test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.4",
    "lodash": "^4.17.21",
    "snyk": "^1.685.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.4",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  },
  "snyk": true
}
